# Example Environment Configuration
# Copy this file to .env.development for local development

# API Base URL - backend server address
VITE_API_BASE_URL=http://localhost:9001

# WebSocket Base URL - for real-time connections
VITE_WS_BASE_URL=ws://localhost:9001

# Frontend development server port
VITE_PORT=4001

# Enable API request logging (true/false)
VITE_API_LOGGING=true
